<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane style="-fx-background-color: #E0FFFF;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.12" fx:controller="tserkovnikov.com.cleanplanetapp.controller.ServiceHistoryController">
    <VBox alignment="CENTER" prefHeight="311.0" prefWidth="518.0" spacing="10">
        <Label fx:id="partnerNameLabel" style="-fx-font-size: 18px; -fx-font-weight: bold;" text="История услуг партнёра" />

        <TableView fx:id="historyTable" prefHeight="500" prefWidth="700" style="-fx-background-color: #E0FFFF;">
            <columns>
                <TableColumn fx:id="serviceNameColumn" prefWidth="250" text="Наименование услуги" />
                <TableColumn fx:id="quantityColumn" prefWidth="100" text="Количество" />
                <TableColumn fx:id="dateColumn" prefWidth="150" text="Дата выполнения" />
            </columns>
        </TableView>

        <Button onAction="#onBack" style="-fx-background-color: #00CED1;" text="Назад" textFill="WHITE" />
    </VBox>
</AnchorPane>
